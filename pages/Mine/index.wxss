.topnav image{
    width: 100vw;
    z-index: 1;
    height:320rpx;
    position: fixed;
}
.mine{
    position: relative;
    width: 85vw;
    left:3.5vw;
    top:160rpx;
    height:200rpx;
    background: rgb(144, 214, 255);
    border-radius: 30rpx;
    z-index: 999;
    padding-left: 60rpx;
    padding-top: 70rpx;
}
.content{
    position: relative;
    width: 93vw;
    left:3.5vw;
    display: flex;
    flex-direction:row;
    justify-content: center;
    align-items: center;
    top:200rpx;
    height:210rpx;
    border-radius: 30rpx;
    z-index: 999;
}
.left{
    flex:1;
    display: flex;
    flex-direction:column;
    justify-content: center;
    align-items: center;
    background-color: #848080;
    border-radius: 30rpx;
    height:100%;
    margin-right:10px
}
.right{
    display: flex;
    flex-direction:row;
    justify-content: center;
    align-items: center;
    background-color: #fff;
    border-radius: 30rpx;
    height:100%;
    flex:3.5
}
.tag{
    position: absolute;
}
.exit{
    position: fixed;
    width:100vw !important;
    bottom:0
}
.bg{
    width: 100vw;
    height: 100vh;
    background-color: #f3f2f2;
}